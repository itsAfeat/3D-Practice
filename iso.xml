<?xml version="1.0" encoding="utf-8"?>

<iso_project
	image_name="${CD_IMAGE_NAME}.bin"
	cue_sheet="${CD_IMAGE_NAME}.cue"
>
	<track type="data">
	
		<identifiers
			system			="PLAYSTATION"
			volume			="3D_PRACTICE"
			volume_set		="3D_PRACTICE"
			publisher		="BREMSESPOR"
			data_preparer	="PSN00BSDK ${PSN00BSDK_VERSION}"
			application		="PLAYSTATION"
			copyright		="README.TXT;1"
		/>
		
		<license file="${PROJECT_SOURCE_DIR}/LICENSEE.DAT" />
		<directory_tree>
			<file name="SYSTEM.CNF"		type="data" source="${PROJECT_SOURCE_DIR}/system.cnf" />
			<file name="GAME.EXE"		type="data" source="game.exe" />
			<file name="STRANGE.TIM"	type="data" source="${PROJECT_SOURCE_DIR}/TIMs/strange.tim" />
			<file name="CUBE.MDL"		type="data" source="${PROJECT_SOURCE_DIR}/MDLs/cubeQuad.mdl" />
			<dummy sectors="1024"/>
		</directory_tree>
	</track>
	
</iso_project>
